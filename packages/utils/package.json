{
  "name": "@tulx/utils",
  "version": "0.0.4",
  "description": "Utility function library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haykavanesyan/tulx.git",
    "directory": "packages/utils"
  },
  "keywords": [
    "utility",
    "utils",
    "lodash",
    "helpers",
    "typescript",
    "esm",
    "tree-shakeable",
    "zero-dependencies"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts",
        "default": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pretest": "pnpm build",
    "test": "vitest",
    "pretest:run": "pnpm build",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "pretest:coverage": "pnpm build",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@tulx/config": "workspace:*",
    "@vitest/ui": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.0",
    "vitest": "^2.0.0"
  }
}
