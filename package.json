{
  "name": "utilify",
  "version": "0.1.0",
  "private": true,
  "description": "Utility library with documentation website",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:run": "turbo run test:run",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "type-check": "turbo run type-check",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:status": "changeset status",
    "version": "pnpm changeset:version && pnpm install",
    "release": "pnpm build && pnpm test:run"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "prettier": "^3.2.5",
    "turbo": "^2.1.3",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
